create table projects(
id int not null,
skill_project int not null,
foreign key(skill_project) references skills(id)
on delete cascade
);

 desc projects;
+---------------+---------+------+-----+---------+-------+
| Field         | Type    | Null | Key | Default | Extra |
+---------------+---------+------+-----+---------+-------+
| id            | int(11) | NO   |     | NULL    |       |
| skill_project | int(11) | NO   | MUL | NULL    |       |
+---------------+---------+------+-----+---------+-------+
2 rows in set (0.01 sec)


